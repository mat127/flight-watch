{
  "name": "flight-watch",
  "version": "1.0.0",
  "description": "",
  "main": "build/gcf.js",
  "scripts": {
    "precompile": "rm -f ./build/*",
    "compile": "npx tsc",
    "compile-watch": "npx tsc --watch",
    "postcompile": "cp .puppeteerrc.cjs build",
    "gcp-build": "npm run compile",
    "postgcp-build": "npx puppeteer browsers install chrome",
    "gcf-deploy": "gcloud functions deploy flight-watch --gen2 --memory=1024M --runtime=nodejs20 --entry-point handler --allow-unauthenticated --trigger-http",
    "prelambda-pack": "rm -f lambda.zip",
    "lambda-pack": "zip -j lambda.zip ./build/*.js",
    "postlambda-pack": "zip -r lambda.zip ./node_modules/ package.json",
    "lambda-deploy": "aws lambda update-function-code --function-name flight-watch --zip-file fileb://lambda.zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mat127/flight-watch.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mat127/flight-watch/issues"
  },
  "homepage": "https://github.com/mat127/flight-watch#readme",
  "dependencies": {
    "axios": "^1.6.7",
    "pg": "^8.11.3",
    "puppeteer": "^22.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@types/aws-lambda": "^8.10.133",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/tough-cookie": "^4.0.5",
    "typescript": "^5.3.3"
  }
}
